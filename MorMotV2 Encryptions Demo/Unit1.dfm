    object TabSheet4: TTabSheet
      Caption = 'About & Info'
      ImageIndex = 3
      object Memo1: TMemo
        Left = 16
        Top = 16
        Width = 825
        Height = 585
        Lines.Strings = (
          'mORMot V2 Comprehensive Cryptography Demo'
          ''
          
            'This application demonstrates the extensive cryptographic capabi' +
            'lities of the mORMot V2 framework.'
          ''
          'SYMMETRIC ENCRYPTION MODES:'
          
            '- AES-ECB: Electronic Codebook (NOT RECOMMENDED for production u' +
            'se)'
          '- AES-CBC: Cipher Block Chaining (widely used, requires padding)'
          '- AES-CFB: Cipher Feedback (stream cipher mode, no padding)'
          '- AES-OFB: Output Feedback (stream cipher mode, no padding)'
          '- AES-CTR: Counter Mode (fast, parallelizable, no padding)'
          
            '- AES-GCM: Galois/Counter Mode (AEAD - Authentication + Encrypti' +
            'on)'
          '- AES-CFC: mORMot CFB + CRC32C (custom AEAD mode)'
          '- AES-OFC: mORMot OFB + CRC32C (custom AEAD mode)'
          '- AES-CTC: mORMot CTR + CRC32C (custom AEAD mode)'
          ''
          'KEY SIZES SUPPORTED:'
          '- 128-bit (16 bytes)'
          '- 192-bit (24 bytes)'
          '- 256-bit (32 bytes)'
          ''
          'HASH FUNCTIONS:'
          '- MD5 (deprecated, for compatibility only)'
          '- SHA-1 (deprecated, for compatibility only)'
          '- SHA-256 (recommended)'
          '- SHA-384'
          '- SHA-512'
          '- SHA-3-256'
          '- SHA-3-512'
          ''
          'ADDITIONAL FEATURES:'
          '- HMAC-SHA256 for message authentication'
          '- PBKDF2 for secure key derivation from passwords'
          '- Cryptographically secure random number generation'
          '- Hardware AES-NI acceleration when available'
          '- PKCS#7 padding for block modes'
          '- Base64 and Hexadecimal encoding'
          ''
          'SECURITY RECOMMENDATIONS:'
          
            '1. Use AES-256 with GCM, CFC, OFC, or CTC modes for new applicat' +
            'ions'
